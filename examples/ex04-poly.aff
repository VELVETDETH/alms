(* Polymorphic version: The C Module, Corrected *)

module[A] ap : all 'a 'b. ('a -o 'b) -> 'a -o 'b =
  \'a 'b (f: 'a -o 'b) (x : 'a). f x

module[C] inc2 : int -> int =
  \y: int.
    let g = ap[int,int] (\z: int. add z 1) in
    let h = ap[int,int] (\z: int. add z 1) in
      h (g y)

in print[int] (inc2 5)
