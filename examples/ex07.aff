(* An Interface Module Intervenes *)

let[C] ap : (int -> int) -> int -> int =
  fun f: (int -> int) ->
    fun x: int ->
      f x

let interface iap :> (int -o int) -> int -o int = ap

let[A] inc : int -> int =
  fun y: int ->
    (fun g: (int -o int) ->
       iap g y)
    (fun z: int -> z + 1)

in print[int] (inc 5)
