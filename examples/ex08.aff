(* A Lying Interface Module (blame iap2) *)

let[C] ap : (int -> int) -> int -> int =
  \f: int -> int.
    \x: int.
      f (f x)    (* f is used twice here, despite what iap2 claims *)

let interface iap2 :> (int -o int) -> int -o int = ap

let[A] inc : int -> int =
  \y: int.
    (\g: int -o int.
       iap2 g y)
    (\z: int. z + 1)

in print[int] (inc 5)
