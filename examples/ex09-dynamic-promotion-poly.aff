(* Polymorphic version: A Dynamic Promotion Intervenes (like ex7.aff) *)

let[C] ap : all 'a. ('a -> 'a) -> 'a -> 'a =
  fun 'a ->
    fun f: ('a -> 'a) ->
      fun x: 'a ->
        f x

let[A] inc : int -> int =
  fun y: int ->
    (fun g: (int -o int) ->
       (ap[int] :  (int -> int) -> int -> int
                :> (int -o int) -> int -o int) g y)
    (fun z: int -> z + 1)

in print[int] (inc 5)
